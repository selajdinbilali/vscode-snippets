{
    "array line": {
		"prefix": "ss",
		"body": [
			"'$1' => $0,"
		]
	},
    "nice echo": {
		"prefix": "ne",
		"body": [
			"echo '$1' . ' : ' . $1 . ' ' . PHP_EOL;$0"
		]
	},
       "public test method": {
        "prefix": "test",
        "body": [
            "public function test_${1:verb}()",
            "{",
            "\t$0",
            "}",
        ],
    },
    "public function": {
		"prefix": "pf",
		"body": [
            "public function ${1:verb}($2)",
            "{",
            "\t$0",
            "}",
		]
	},
    "laravel comment but smaller": {
        "prefix": "lbox",
        "body": [
            "/*----------------------------------------------------------------------",
            "| $0",
            "|---------------------------------------------------------------------*/",
        ]
    },
}
